<o:Operation Id="o1941">
<a:ObjectID>197ED444-E43F-40C1-83FC-26A4035A9BD2</a:ObjectID>
<a:Name>Creating</a:Name>
<a:Code>Creating</a:Code>
<a:CreationDate>1340376450</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
