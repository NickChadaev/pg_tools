         tablesH       = inDialog.getVector ();
      }                      
     else { 
           inheritActive = false;
           inheritString = &quot;&quot;;
           tablesH = new Vector();
      }
   }

   /****
   if ( e.getActionCommand().equals ( &quot;BUT-CONST&quot; )) {

       Constraint winCons = new Constraint ( CreateTable.this, idiom, ConstNames, ConstDef );

       if ( winCons.isWellDone () ) {

