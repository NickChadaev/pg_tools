<o:Operation Id="o1694">
<a:ObjectID>DE579526-0A6F-4927-8A7D-84844772CE86</a:ObjectID>
<a:Name>clearSpaces</a:Name>
<a:Code>clearSpaces</a:Code>
<a:CreationDate>1335896419</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1335896457</a:ModificationDate>
<a:Modifier>Андрей Жовнер</a:Modifier>
<a:ReturnType>String</a:ReturnType>
<a:TemplateBody> {
   String valid = &quot;&quot;;

   if((inS.indexOf(&quot;  &quot;) != -1) || (inS.indexOf(&quot;\t&quot;) != -1) || (inS.indexOf(&quot;\n&quot; ) != -1))
    {
      int x = 0;
      while (x &lt; inS.length())
       {
  char w = inS.charAt(x);
  if (w == &#39;\t&#39;  &amp;&amp; ( inS.charAt(x+1) != &#39; &#39;) &amp;&amp; ( inS.charAt(x+1) != &#39;\t&#39;) &amp;&amp; ( inS.charAt(x+1) != &#39;\n&#39; ))
           valid = valid + &quot; &quot;;
         if (w == &#39;\t&#39;  &amp;&amp; (( inS.charAt(x+1) == &#39; &#39;) || ( inS.charAt(x+1) == &#39;\t&#39;) || ( inS.charAt(x+1) == &#39;\n&#39;)))
           valid = valid + &quot;&quot;;
  if ( w == &#39; &#39; &amp;&amp; ( inS.charAt(x+1) != &#39; &#39;) &amp;&amp; ( inS.charAt(x+1) != &#39;\t&#39;) &amp;&amp; ( inS.charAt(x+1) != &#39;\n&#39;))
           valid = valid + w;
  if (w == &#39; &#39;  &amp;&amp; (( inS.charAt(x+1) == &#39; &#39;) || ( inS.charAt(x+1) == &#39;\t&#39;) || ( inS.charAt(x+1) != &#39;\n&#39;)))
    valid = valid + &quot;&quot;;
  if ( w == &#39;\n&#39; &amp;&amp; ( inS.charAt(x+1) != &#39; &#39;) &amp;&amp; ( inS.charAt(x+1) != &#39;\t&#39;) &amp;&amp; ( inS.charAt(x+1) != &#39;\n&#39; ))
           valid = valid + &quot; &quot;;
  if ( w == &#39;\n&#39; &amp;&amp; (( inS.charAt(x+1) == &#39; &#39;) || ( inS.charAt(x+1) == &#39;\t&#39;) || ( inS.charAt(x+1) == &#39;\n&#39; )))
    valid = valid + &quot;&quot;;
  if ( w != &#39; &#39; &amp;&amp; w != &#39;\t&#39; &amp;&amp; w != &#39;\n&#39;)
    valid = valid + w;
  x++;
       }

   }
  else
    valid = inS;

  while(valid.startsWith(&quot; &quot;))
     valid = valid.substring(1,valid.length());

 return valid;
}
</a:TemplateBody>
