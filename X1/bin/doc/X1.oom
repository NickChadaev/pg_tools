<a:Modifier>Андрей Жовнер</a:Modifier>
<a:ReturnType>int</a:ReturnType>
<a:TemplateBody>{

    double fl = ((double)rIM)/50;
    String div = &quot;&quot; + fl;

    int nP = rIM/50;

    if ( div.indexOf (&quot;.&quot;) != -1 ) {
        String str = div.substring(div.indexOf(&quot;.&quot;)+1,div.length());
        if (!str.equals(&quot;0&quot;))
             nP++;
     }
  return nP;
}
</a:TemplateBody>
