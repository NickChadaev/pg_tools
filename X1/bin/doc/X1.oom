              if ( CurrentTable.indexOf ( cSPACE ) == -1 ) { 
                  if ( fieldsN.size() == 0 ) {
                		 	JOptionPane.showMessageDialog ( CreateTable.this, idiom.getWord (&quot;NOFCR&quot;),
                		 			idiom.getWord (&quot;ERROR!&quot;), JOptionPane.ERROR_MESSAGE
                		 	);
