<o:Operation Id="o219">
<a:ObjectID>39DBB992-B9CF-44C6-B4B2-573F5B9318AE</a:ObjectID>
<a:Name>PropertiesTable</a:Name>
<a:Code>PropertiesTable</a:Code>
<a:CreationDate>1335888273</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1335888345</a:ModificationDate>
