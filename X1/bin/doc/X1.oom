<a:Code>SetLenPrec</a:Code>
<a:CreationDate>1335888272</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1335888345</a:ModificationDate>
<a:Modifier>Андрей Жовнер</a:Modifier>
<a:Comment> Check selection of  type&#39;s combo box.
 2012-02-21 Nick


 SetLenPrec</a:Comment>
<a:ReturnType>void</a:ReturnType>
<a:Operation.Visibility>-</a:Operation.Visibility>
<a:TemplateBody>{
 
 // ----- typeIndex 11 3 2 1
 if ( (typeIndex == 1) || ( (typeIndex == 3) &amp;&amp; (! p_newSel.equals(ChkType.cTEXT)) ) ||
(typeIndex == 11)   
    ) 
  {
     LengthField.setEditable ( true );
     LengthField.setEnabled  ( true );
    
     if ( (typeIndex == 1) || (typeIndex == 2) )	   
                  {
 	   		          PrecField.setEditable ( true );
 	   	              PrecField.setEnabled  ( true );
                  }
  } 
 else
      {
       LengthField.setEditable ( false );
       LengthField.setEnabled  ( false );
       //
       PrecField.setEditable ( false );
       PrecField.setEnabled  ( false );
      }
}
