   else readLang = new ConfigFileReader ( configPath, 1 );
 
 file = new File ( xpgHome + cLOG_CATALOG );    // + System.getProperty ( cSYS_FILE_SEP ) 
 if ( !file.exists () ) file.mkdir();
 
 file = new File ( xpgHome + cQR_CATALOG );     // + System.getProperty ( cSYS_FILE_SEP )
 if ( !file.exists () ) file.mkdir();
 
 file = new File ( xpgHome + cREP_CATALOG );    // + System.getProperty ( cSYS_FILE_SEP ) 
