<a:TemplateBody>{

 int val;
 String counting = &quot;SELECT count(*) FROM &quot; + TableN + &quot;;&quot;;
 Vector result = konn.TableQuery(counting);
 Vector value = (Vector) result.elementAt(0);

 try {
      Integer entero = (Integer) value.elementAt(0);
      val = entero.intValue();
  }
