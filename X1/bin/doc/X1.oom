    int keyCode = e.getKeyCode();
    String keySelected = KeyEvent.getKeyText(keyCode); 

    if (keySelected.equals(&quot;Delete&quot;)) {

        dropRecords();
        return;
     }

    if (keySelected.equals(&quot;Insert&quot;)) {

