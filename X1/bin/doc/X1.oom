<a:Name>getOrder</a:Name>
<a:Code>getOrder</a:Code>
<a:CreationDate>1335896113</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1335896282</a:ModificationDate>
<a:Modifier>Андрей Жовнер</a:Modifier>
<a:ReturnType>String</a:ReturnType>
<a:TemplateBody>{

   String where = text.toLowerCase(); 
   String order = &quot;&quot;;

   int index = where.indexOf(&quot;order by&quot;);

   if (index != -1) {

       order = where.substring(index + 9,where.length());
       int k = text.length();

       char[] characters = order.toCharArray();

       for (int i=0;i&lt;characters.length;i++) {
            if (characters[i] == &#39; &#39;) {
                k = i;
                break;
             } 
        } 
       
       int pos = index + 9;
       k += pos;

       String tmp = text.substring(pos,k);

       if (order.indexOf(&quot; asc&quot;) != -1 )
           tmp += &quot; ASC&quot;;

       if (order.indexOf(&quot; desc&quot;) != -1 )
           tmp += &quot; DESC&quot;; 

       order = tmp;
    }

   return order;
 }
