<o:Operation Id="o1893">
<a:ObjectID>BEC54861-45BC-4FA7-8EAE-75D1E7197847</a:ObjectID>
<a:Name>CreateDB</a:Name>
<a:Code>CreateDB</a:Code>
<a:CreationDate>1340376450</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
