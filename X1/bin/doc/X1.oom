<o:Operation Id="o2097">
<a:ObjectID>819212D0-45EC-4C54-8373-BACCBA6E8596</a:ObjectID>
<a:Name>DropTable</a:Name>
<a:Code>DropTable</a:Code>
<a:CreationDate>1340376450</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1340376452</a:ModificationDate>
