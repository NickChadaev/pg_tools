  if ( e.getActionCommand ().equals ( &quot;CMB-REF&quot; )) {

      String tableN = (String) cmbReferences.getSelectedItem ();

      if ( !tableN.startsWith (idiom.getWord (&quot;NOTABLES&quot;) ) ) {

          Vector listTables = new Vector();
          listTables.addElement ( tableN );
          
          // 2011-11-13 Nick
