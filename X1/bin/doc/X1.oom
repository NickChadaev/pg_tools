<o:Operation Id="o1160">
<a:ObjectID>BB3D585A-E02C-4FB3-8DA1-EB1AAB4FF17A</a:ObjectID>
<a:Name>getTypeCode</a:Name>
<a:Code>getTypeCode</a:Code>
<a:CreationDate>1335896114</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1335896282</a:ModificationDate>
<a:Modifier>Андрей Жовнер</a:Modifier>
<a:ReturnType>int</a:ReturnType>
<a:Operation.Visibility>*</a:Operation.Visibility>
<a:TemplateBody> {
  if ( typeStr.startsWith (&quot;varchar&quot;) || typeStr.startsWith (&quot;char&quot;) || 
	typeStr.startsWith (&quot;text&quot;)    || typeStr.startsWith (&quot;name&quot;)  || 
	typeStr.startsWith (&quot;date&quot;)    || typeStr.startsWith (&quot;time&quot;)
  )
      return 1;

  if ( typeStr.startsWith (&quot;int&quot;)  || typeStr.equals (&quot;serial&quot;) || 
	typeStr.equals (&quot;smallint&quot;) || typeStr.equals (&quot;real&quot;)   || 
	typeStr.equals (&quot;double&quot;)
  )
      return 2;

  if ( typeStr.startsWith (&quot;bool&quot;) ) return 3;
  else return 4;
}
</a:TemplateBody>
