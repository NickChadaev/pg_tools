  if ( e.getActionCommand ().equals (&quot;SKEY&quot;) ) {

      // if ( isKey.isSelected () ) setAvalaibleKey ( true );
      //  else setAvalaibleKey ( false );

	  setAvalaibleKey ( isKey.isSelected () );
      combosPanel.updateUI ();
