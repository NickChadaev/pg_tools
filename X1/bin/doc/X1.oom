<a:ObjectID>ED5D5C41-503D-43D4-A4FA-D9A2A41D8165</a:ObjectID>
<a:Name>BuildSQLRecords</a:Name>
<a:Code>BuildSQLRecords</a:Code>
<a:CreationDate>1340376450</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1340376452</a:ModificationDate>
<a:Modifier>Андрей Жовнер</a:Modifier>
<a:ReturnType>String</a:ReturnType>
<a:Operation.Visibility>*</a:Operation.Visibility>
<a:TemplateBody>{
  Vector data = dbDump.TableQuery(&quot;select * from &quot; + table);
  Vector col = dbDump.TableHeader;
  String sql = &quot;&quot;;

  if(!dbDump.queryFail())
    {
     for(int p=0;p&lt;data.size();p++)
      {
       sql += &quot;INSERT INTO &quot; + table + &quot; VALUES(&quot;;
       Vector tempo = (Vector) data.elementAt(p);
       int numCol = tempo.size();
       for(int j=0;j&lt;numCol;j++)
        {
         String colName = (String) col.elementAt(j);
         String type = headT.getType(colName);
         Object o = tempo.elementAt(j);

         if(type.startsWith(&quot;varchar&quot;) || type.startsWith(&quot;char&quot;) || type.startsWith(&quot;text&quot;) || type.startsWith(&quot;name&quot;) || type.startsWith(&quot;date&quot;) || type.startsWith(&quot;time&quot;))
            sql += &quot;&#39;&quot; + o.toString() + &quot;&#39;&quot;; 
         else
            sql += o.toString();

         if(j &lt; (numCol - 1))
           sql += &quot;,&quot;;
        }
       sql += &quot;);\n&quot;;
      }
    }

  return sql;
}
