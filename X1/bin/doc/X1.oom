   */

  if ( e.getActionCommand ().equals ( &quot;CHECK-DEFV&quot; )) { 

      // boolean hab = false;

      // if ( CBr3.isSelected() ) hab = true;
      textField6.setEnabled  ( CBr3.isSelected () );
