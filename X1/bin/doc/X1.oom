<a:Name>actionPerformed</a:Name>
<a:Code>actionPerformed</a:Code>
<a:CreationDate>1335892885</a:CreationDate>
<a:Creator>Андрей Жовнер</a:Creator>
<a:ModificationDate>1335893157</a:ModificationDate>
<a:Modifier>Андрей Жовнер</a:Modifier>
<a:ReturnType>void</a:ReturnType>
<a:TemplateBody>{

 if (e.getActionCommand().equals(&quot;CANCEL&quot;)) {

     setVisible(false);
     return;
  }		

 if (e.getActionCommand().equals(&quot;OK&quot;)) {

    if (currentScreen.isSelected()) 
        option = 1;

    if(onMemory.isSelected())
       option = 2;

    if(onTotal.isSelected())
       option = 3;

    wellDone = true;
    setVisible(false);
    return;
  }

}
</a:TemplateBody>
