   	  {                                                 								
     	    Object value = optionPane.getValue();           					                  
     	    if (value == JOptionPane.UNINITIALIZED_VALUE)   								
           {                                              								
            //ignorar reset                            			                          
            return;                                   								
           }                                              							  
     	    if (value.equals(btnString1))
           {
            setVisible(false);   
    }                    
     	    else
           { 
     setVisible(false);
    } 
    	  } 
    }
  });
  setSize(320,200);
}
